@model System.Collections.Generic.List<VendorOrderTracker.Models.Vendor>
<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Vendor List</title>
    <link 
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" 
      rel="stylesheet" 
      integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" 
      crossorigin="anonymous">
  </head>
  <body>
    <p>Here is some stuff.</p>
    <table>
      @foreach (var item in Model)
      {
      <tr>
        <td>@item.VendorName</td>
        <td>@item.VendorDesc</td>
      </tr>
      } 
    </table>
    <p>@Html.ActionLink("Form to add a new vendor.", "AddVendor", "Vendors")</p>
    <p>@Html.ActionLink("Form to add a new order.", "Index", "Orders" )</p>
  </body>
</html>