@model System.Collections.Generic.List<VendorOrderTracker.Models.Vendor>
<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Vendor List</title>
  </head>
  <body>
    <p>Here is some stuff.</p>
    <table>
      @foreach (var item in Model)
      {
      <tr>
        <td>@item.VendorName</td>
        <td>@item.VendorDesc</td>
      </tr>
      @* @Html.DisplayFor(modelItem => item.VendorName) 
      @Html.DisplayFor(modelItem => item.VendorDesc) *@
      } 
    </table>
    
    
  </body>
</html>